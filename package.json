{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:server",
    "build:server": "tsup server/index.ts --format esm --platform node --target node20 --out-dir server-dist --clean",
    "lint": "eslint .",
    "preview": "vite preview",
    "serve": "node server-dist/index.js"
  },
  "dependencies": {
    "express": "^4.21.2",
    "@tonaljs/chord": "^6.1.1",
    "@tonaljs/note": "^4.12.1",
    "clsx": "^2.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/express": "^4.17.21",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
